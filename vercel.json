{
    "builds":[
        {
            "src":"eventpr/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"50mb",
                "runtime":"python3.8.0"
            }
        } 
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"eventpr/wsgi.py"
        }
    ]
}